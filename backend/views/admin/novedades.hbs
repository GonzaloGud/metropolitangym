    <img src="/images/logo.png" alt="gymLogo" />

  </div>
</header>
<h1>METROPOLITAN GYM</h1>

<!--Nav-->

<body>
  <nav class="navMenu">
    <div class="logoutBottom">
      <a href="./login">Log Out</a>
    </div>
    </a>
  </nav>
</body>

<!--List-->
<div class="clientList">

  <div class="listTile">
    <h3>client List</h3>
  </div>

  <div class="addClient">
    <a href="/admin/novedades/agregar">Add Client</a>
  </div>

  <table id="customers">
    <tr>
      <th>Name</th>
      <th>Class</th>
      <th>ID</th>
      <th>/</th>
    </tr>

    {{#each novedades}}
    <tr>
      <td>{{name}}</td>
      <td>{{class}}</td>
      <td>{{id}}</td>
      <td><a href="/admin/novedades/eliminar/{{id}}" class="eliminar"><i class="eliminar"></i>eliminar</a></td>
    </tr>


</div>
{{/each}}